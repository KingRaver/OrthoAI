{
  "name": "orthoai",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --ext .ts,.tsx --fix",
    "type-check": "tsc --noEmit",
    "analyze": "next build && npx @next/bundle-analyzer dist",
    "preview": "next build && next start",
    "chroma-start": "docker start chromadb || docker run -d --name chromadb -p 8000:8000 -v $(pwd)/.data/chroma:/chroma/chroma chromadb/chroma:latest",
    "chroma-stop": "docker stop chromadb"
  },
  "dependencies": {
    "ai": "^6.0.20",
    "better-sqlite3": "^12.5.0",
    "chromadb": "^3.2.0",
    "mathjs": "^15.1.0",
    "next": "16.1.1",
    "openai": "^6.15.0",
    "pyodide": "^0.29.1",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-markdown": "^10.1.0",
    "three": "^0.160.0",
    "undici": "^7.18.2",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^16.1.1",
    "@tailwindcss/postcss": "^4",
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/three": "^0.182.0",
    "eslint": "^9",
    "eslint-config-next": "16.1.1",
    "postcss": "^8.5.1",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}